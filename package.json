{
    "name": "open-in-sketch",
    "displayName": "Open in Sketch",
    "description": "Open current file in Sketch",
    "version": "2.0.0",
    "publisher": "davidbucka",
    "repository": {
        "type": "git",
        "url": "https://github.com/davidbucka/vscode-open-in-sketch.git"
    },
    "icon": "resources/logo.png",
    "keywords": [
        "vscode",
        "svg",
        "extension",
        "open in",
        "sketch"
    ],
    "license": "MIT",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openSketch"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.openSketch",
                "title": "Open in Sketch"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "bugs": {
        "url": "https://github.com/davidbucka/vscode-open-in-sketch/issues"
    },
    "author": {
        "name": "David Bucka",
        "email": "info@davidbucka.com"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "open": "0.0.5",
        "path": "^0.12.7"
    }
}
