    "description": "Open current file in Sketch",
    "version": "0.0.1",
    "publisher": "davidbucka",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onCommand:extension.openSketch"],
